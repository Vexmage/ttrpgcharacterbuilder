@model TTRPG_Character_Builder.Models.LoginViewModel

@{
    ViewData["Title"] = "Login";
}

<h2>Login</h2>
<form method="post" asp-action="Login" asp-controller="User">
    <input asp-for="Username" placeholder="Username" required />
    <input asp-for="Password" placeholder="Password" required />
    <button type="submit">Login</button>
</form>
